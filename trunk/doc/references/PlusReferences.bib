@InProceedings{Khamene2005,
  Title                    = {A Novel Phantom-Less Spatial and Temporal Ultrasound Calibration Method},
  Author                   = {Ali Khamene and Frank Sauer},
  Booktitle                = {MICCAI (2)},
  Year                     = {2005},
  Pages                    = {65-72},

  Abstract                 = {This paper introduces a novel method for ultrasound calibration for both spatial and temporal parameters. The main advantage of this method is that it does not require a phantom, which is usually expensive to fabricate. Furthermore, the method does not require extensive image processing. For spatial calibration, we solve an optimization problem established by a set of equations that relate the orientations of a line (i.e., calibration pointer) to the intersection points appearing in the ultrasound image. The line orientation is provided through calibration of both ends of the calibration pointer. Temporal calibration is achieved by processing of the captured pointer orientations and the corresponding image positions of intersection along with the timing information. The effectiveness of the unified method for both spatial and temporal calibration is apparent from the quality of the 3D reconstructions of a known object.},
  Bibsource                = {DBLP, http://dblp.uni-trier.de},
  Crossref                 = {DBLP:conf/miccai/2005-2},
  Ee                       = {http://dx.doi.org/10.1007/11566489_9},
  File                     = {:Khamene2005.pdf:PDF},
  Owner                    = {lasso},
  Timestamp                = {2012.10.19}
}

