@Article{Rousseau2005,
  Title                    = {Confhusius: a robust and fully automatic calibration method for 3D freehand ultrasound.},
  Author                   = {Rousseau, François and Hellier, Pierre and Barillot, Christian},
  Journal                  = {Med Image Anal},
