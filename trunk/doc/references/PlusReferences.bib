  Owner                    = {lasso},
  Pii                      = {S0301-5629(13)01225-8},
  Pmid                     = {24495435},
  Timestamp                = {2014.02.11},
  Url                      = {http://dx.doi.org/10.1016/j.ultrasmedbio.2013.11.027}
}

